<h2>Available drones</h2>
<ul>
{{#each drones}}
  <li>Name: {{name}}</li>
  <li>Propellers: {{propellers}}</li>
  <li>Max Speed:{{maxSpeed}}</li>
  <form action="/drones/{{this.id}}/delete" method="POST">
  <button>Delete</button>
  </form>
  <a href="/drones/{{this.id}}/edit">Edit</a>
{{/each}}
</ul>

